{
  "name": "Backup S3",
  "version": "2.2.8",
  "slug": "backup_s3",
  "description": "Backup config to S3 bucket",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "once",
  "boot": "auto",
  "init": false,
  "options": {
